{  "extends": "stylelint-config-standard-scss",  "rules": {    "indentation": 1,    "string-quotes": "double",    "no-duplicate-selectors": true,    "color-hex-case": "lower",    "color-hex-length": "short",    "color-named": "never",    "selector-combinator-space-after": "never",    "selector-attribute-operator-space-before": "never",    "declaration-block-trailing-semicolon": "never",    "rule-empty-line-before": "never",    "at-rule-empty-line-before": "never"  }}