{  "env": {    "browser": true,    "node": false,    "mongo": false,    "es6": true  },  "extends": ["airbnb-base"],  "parserOptions": {    "ecmaFeatures": {      "impliedStrict": true    }  },  "rules": {    "no-underscore-dangle": [      "error",      { "allow": ["_id"] }    ],    "quotes": [2, "double", { "avoidEscape": true }],    "no-console": 0  }}